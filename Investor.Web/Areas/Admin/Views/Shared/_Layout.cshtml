<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>Metronic Admin Theme #1 | Admin Dashboard</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1" name="viewport" />
    <meta content="Preview page of Metronic Admin Theme #1 for statistics, charts, recent events and reports" name="description" />
    <meta content="" name="author" />

    <link href="http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700&subset=all" rel="stylesheet" type="text/css" />
    <link href="~/css/metronic/bootstrap.min.css" rel="stylesheet" />
    <link href="~/css/metronic/simple-line-icons.min.css" rel="stylesheet" />
    <link href="~/css/metronic/components-rounded.min.css" rel="stylesheet" />
    <link href="~/css/metronic/layout.min.css" rel="stylesheet" />
    <link href="~/css/metronic/darkblue.min.css" rel="stylesheet" />
    <link href="~/css/metronic/bootstrap-fileinput.css" rel="stylesheet" />
    <link href="~/css/metronic/bootstrap-tagsinput-typeahead.css" rel="stylesheet" />
    <link href="~/css/metronic/bootstrap-tagsinput.css" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href="//cdn.datatables.net/1.10.16/css/jquery.dataTables.css">
</head>

<body class="page-header-fixed page-sidebar-closed-hide-logo page-content-white">
    <div class="page-wrapper">
        <header>
            @Html.Partial("_HeaderSection")
        </header>
        <div class="page-container">
            <div>
                @Html.Partial("_SideBarSection")
            </div>
            <div id="container">
                @RenderBody()
            </div>


        </div>
        <footer>
            @Html.Partial("_FooterSection")
        </footer>
    </div>
    @*admin-content scripts*@

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-migrate/3.0.1/jquery-migrate.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.18.1/moment.js"></script>


    <script src="~/js/metronic/bootstrap.min.js"></script>
    <script src="~/js/metronic/app.min.js"></script>
    <script src="~/js/metronic/layout.min.js"></script>
    <script type="text/javascript" charset="utf8" src="//cdn.datatables.net/1.10.16/js/jquery.dataTables.js"></script>

    <script src="~/js/tinymce/tinymce.min.js"></script>
    <script src="~/js/tinymce/themes/modern/theme.min.js" type="text/javascript"></script>
    <script src="~/js/tinymce/editor.js" type="text/javascript"></script>
    <script src="~/js/metronic/bootstrap-fileinput.js"></script>
    <script src="~/js/metronic/bootstrap-tagsinput.js"></script>
    <script src="~/js/typeahead/typeahead.bundle.js"></script>

    <script src="~/js/admin/content/new-post.js"></script>
    <script src="~/js/admin/content/news.js"></script>
    <script src="~/js/admin/content/tags.js"></script>
    <script src="~/js/admin/content/blogs.js"></script>


</body>
</html>
